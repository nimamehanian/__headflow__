*
  margin 0px
  padding 0px

html, body
  color $storm
  font-family 'Open Sans', sans-serif
  outline 0px
  overflow hidden

h1, h2, h3, h4, h5, h6
  margin 0px
  padding 0px

a
  &:active, &:hover, &:link, &:visited
    color inherit
    text-decoration none

p
  display inline

ul
  padding-left 0px

li
  list-style-type none
  text-align left

:focus
  outline none
