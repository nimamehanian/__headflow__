{
  "name": "headflow",
  "private": true,
  "version": "1.0.0",
  "description": "headflow",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ]
  },
  "scripts": {
    "cp-favicon": "cp src/images/favicon.png dist",
    "cp-index": "cp src/index.html dist",
    "cp-manifest": "cp src/manifest.json dist",
    "copy": "npm run cp-index && npm run cp-manifest && npm run cp-favicon && webpack",
    "prep": "rm -rf dist && mkdir dist && npm run copy",
    "start": "npm run prep && webpack-dev-server --inline",
    "deploy": "npm run prep && NODE_ENV=production node node_modules/.bin/webpack && firebase deploy",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.5",
    "deep-equal": "1.0.1",
    "firebase": "4.5.0",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "lodash": "4.17.4",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-addons-perf": "15.4.2",
    "react-dom": "16.0.0",
    "react-motion": "0.5.2",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "slate": "0.25.3"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.7",
    "eslint": "4.8.0",
    "eslint-config-airbnb": "16.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "1.1.5",
    "path": "0.12.7",
    "react-hot-loader": "1.3.1",
    "redux-devtools": "3.4.0",
    "style-loader": "0.19.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.6.2",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.1"
  }
}
