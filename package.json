{
  "name": "headflow",
  "private": true,
  "version": "1.0.0",
  "description": "headflow",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ]
  },
  "scripts": {
    "cp-favicon": "cp src/images/favicon.png dist",
    "cp-index": "cp src/index.html dist",
    "copy": "npm run cp-index && npm run cp-favicon && webpack",
    "prep": "rm -rf dist && mkdir dist && npm run copy",
    "start": "npm run prep && webpack-dev-server --inline",
    "deploy": "NODE_ENV=production node node_modules/.bin/webpack && firebase deploy"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "classnames": "2.2.5",
    "deep-equal": "1.0.1",
    "draft-js": "0.10.0",
    "firebase": "3.9.0",
    "history": "4.6.1",
    "immutable": "3.8.1",
    "lodash": "4.17.4",
    "prop-types": "15.5.8",
    "react": "15.5.4",
    "react-addons-perf": "15.4.2",
    "react-dom": "15.5.4",
    "react-motion": "0.5.0",
    "react-redux": "5.0.4",
    "react-router-dom": "4.1.1",
    "react-router-redux": "5.0.0-alpha.4",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "slate": "0.19.21"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "file-loader": "0.11.1",
    "path": "0.12.7",
    "react-hot-loader": "1.3.1",
    "redux-devtools": "3.4.0",
    "style-loader": "0.17.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.5.8",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.5"
  }
}
