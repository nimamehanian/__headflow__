{
  "name": "headflow",
  "private": true,
  "version": "1.0.0",
  "description": "headflow",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "scripts": {
    "cp-favicon": "cp src/images/favicon.png dist",
    "cp-index": "cp src/index.html dist",
    "copy": "npm run cp-index && npm run cp-favicon && webpack",
    "prep": "rm -rf dist && mkdir dist && npm run copy",
    "start": "npm run prep && webpack-dev-server --inline",
    "deploy": "NODE_ENV=production node node_modules/.bin/webpack && firebase deploy"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "lodash": "4.17.4",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-motion": "0.4.7",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-object-rest-spread": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "css-loader": "0.26.1",
    "eslint": "3.14.1",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "file-loader": "0.10.0",
    "path": "0.12.7",
    "react-hot-loader": "1.3.1",
    "redux-devtools": "3.3.2",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  }
}
